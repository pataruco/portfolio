<div class="container">
  <div class="row">
    <% @projects.each do |project| %>
      <div class="col-md-6">
        <h3><%= link_to project.name, project_path(project) %></h3>
        <img src="<%= project.img  %>" alt="<%= project.name  %>" class="img-responsive thumbnail">
        <p><%= project.technologies %></p>
      </div>
    <% end %>
  </div>
</div>





  